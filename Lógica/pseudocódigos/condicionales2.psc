Proceso condicionales2
	
	Definir gradosC Como Real;
	
	Imprimir "Escribe los grados centígrados:";
	Leer gradosC;
	
	Si ( gradosC>=35 y gradosC<=40 ) o ( gradosC>=50 y gradosC<=55 ) Entonces
		Imprimir "Temperatura dentro del rango";
	SiNo
		Imprimir "Temperatura fuera de rango";
	FinSi
	
FinProceso
